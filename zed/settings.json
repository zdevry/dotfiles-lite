{
  "features": {
    "inline_completion_provider": "none",
    "copilot": false
  },
  "notification_panel": {
    "button": false
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "assistant": {
    "button": false,
    "version": "2",
    "enabled": false
  },
  "collaboration_panel": {
    "button": false
  },
  "chat_panel": {
    "button": false
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false,
    "selections_menu": false
  },
  "ui_font_size": 16,
  "ui_font_family": "Iosevka Fixed",
  "ui_font_weight": 500,
  "buffer_font_size": 21,
  "buffer_font_family": "Iosevka Fixed",
  "buffer_font_weight": 500,
  "buffer_line_height": {
    "custom": 1.47
  },
  "base_keymap": "VSCode",
  "terminal": {
    "shell": {
      "program": "fish"
    },
    "font_family": "Iosevka Fixed",
    "font_weight": 500,
    "font_size": 20,
    "line_height": "standard",
    "working_directory": "current_project_directory",
    "detect_venv": "off"
  },
  "auto_update": false,
  "theme": "Personal Theme",
  "lsp": {
    "clangd": {
      "binary": {
        "path": "/usr/bin/clangd",
        "arguments": ["-header-insertion=never"]
      }
    }
  }
}
